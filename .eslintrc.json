{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "eol-last": 0,
    "import/prefer-default-export": 0,
    "no-trailing-spaces": 0,
    "max-len": 0,
    "no-unused-vars": 1,
    "no-multiple-empty-lines": 0,
    "padded-blocks": 1,
    "func-names": 0,
    "no-console": 0,
    "react/destructuring-assignment": 1
  }
}